<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <remove name="python" />
            <add name="python" path="*.py" verb="*" modules="CgiModule" scriptProcessor="D:\Python27\python.exe %s %s" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
